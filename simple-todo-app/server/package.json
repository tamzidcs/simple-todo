{
  "name": "todolist_server",
  "version": "1.0.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "NODE_ENV=development concurrently \"npx tsc  --watch\" \"nodemon  -q dist/server.js\"",
    "start:debug:ts": "NODE_ENV=development node --inspect --require ts-node/register src/server.ts",
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "db": "NODE_ENV=development sequelize db:create && NODE_ENV=development sequelize db:migrate",
    "db:test": "NODE_ENV=test sequelize db:create && NODE_ENV=test sequelize db:migrate"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-errors": "~1.6.3",
    "http-status": "^1.6.2",
    "pg": "^8.9.0",
    "supertest": "^6.3.3",
    "ts-node-register": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.12",
    "babel-jest": "^29.5.0",
    "bcrypt": "^5.1.0",
    "concurrently": "^7.6.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.2",
    "sequelize": "^6.28.2",
    "sequelize-cli": "^6.6.0",
    "sequelize-typescript": "^2.1.5",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app/jest"
    ]
  }
}
